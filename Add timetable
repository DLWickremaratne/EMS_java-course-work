    //create objectname shedulelist
    public ArrayList<Shedules> sheduleList(){
    ArrayList<Shedules> sheduleList = new ArrayList<>();
    try{
            Class.forName("com.microsoft.sqlserver.jdbc.SQLServerDriver");
            String url="jdbc:sqlserver://localhost:1433;databaseName=NSBM;user=dhanith;password=lakjaya";
            Connection con = DriverManager.getConnection(url);
            
            String query1 ="SELECT * FROM shedule";
            Statement st= con.createStatement();
            ResultSet rs= st.executeQuery(query1);
            //object
            Shedules shedules;
            
            while(rs.next()){
                
                shedules =new Shedules(rs.getInt("Addno"), rs.getFloat("batch"), rs.getString("year"), rs.getString("faculty"), rs.getString("module"), rs.getString("lecturer"), rs.getString("time"), rs.getString("date"),  rs.getString("duration"),  rs.getString("hall") );
                //add shedules in sheduleList
                sheduleList.add(shedules); 
            
            }
            
            
            
            
        }
        catch(Exception ex){
            JOptionPane.showMessageDialog(this, ex.getMessage());
        }
        return sheduleList;
        //this stores the data that is from database
    }
    
    //display data into datatable
    //ude default table model
    public void show_shedule(){ //
        ArrayList<Shedules> list = sheduleList();
        DefaultTableModel model = (DefaultTableModel)jTable1.getModel();  //use type casting,create model object
        
        //create array to store row data 
        
        Object[] row = new Object[10];
        for (int i=0;i<list.size();i++){
            row[0]=list.get(i).getAddno();
            row[1]=list.get(i).getbatch();
            row[2]=list.get(i).getyear();
            row[3]=list.get(i).getfaculty();
            row[4]=list.get(i).getmodule();
            row[5]=list.get(i).getlecturer();
            row[6]=list.get(i).gettime();
            row[7]=list.get(i).getdate();
            row[8]=list.get(i).getduration();
            row[9]=list.get(i).gethall();
            
            
            //add row in model
            model.addRow(row);
        }
    }

