    private void jButton5ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        try{
            Class.forName("com.microsoft.sqlserver.jdbc.SQLServerDriver");
            String url="jdbc:sqlserver://localhost:1433;databaseName=NSBM;user=dhanith;password=lakjaya";
            Connection con = DriverManager.getConnection(url);
            int row = jTable1.getSelectedRow();
            String value = (jTable1.getModel().getValueAt(row, 0).toString());
            
            String query ="UPDATE shedule SET batch=?, year=?, faculty=?, module=?, lecturer=?, time=?, date=?, duration=?, hall=? where Addno="+value;
            PreparedStatement pst = con.prepareStatement(query);
            
            
            String bat=jComboBox1.getSelectedItem().toString();
            pst.setString(1, bat);
            
            String yea=jComboBox2.getSelectedItem().toString();
            pst.setString(2, yea);
            
            String fac=jComboBox3.getSelectedItem().toString();
            pst.setString(3, fac);
            
            pst.setString(4, jTextField2.getText() );
            
            pst.setString(5, jTextField5.getText() );
            
            pst.setString(6, jTextField3.getText() );
            
            SimpleDateFormat sdf = new SimpleDateFormat("yyyy-MM-dd");
            String date =sdf.format(jDateChooser1.getDate()) ;
            pst.setString(7,date);
            
            
            pst.setString(8, jTextField4.getText() );
            
            pst.setString(9, jTextField6.getText() );
            
           
            
            pst.executeUpdate();
            
            DefaultTableModel model = (DefaultTableModel)jTable1.getModel();
            model.setRowCount(0);
            show_shedule();
            
            
            
            JOptionPane.showMessageDialog(null, "Updated Sucessfully");
       }
       catch(Exception ex){
            JOptionPane.showMessageDialog(this, ex.getMessage());
        }
        
        
    }                                        

    private void jButton4ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        int opt =JOptionPane.showConfirmDialog(null, "Are You to Delete", "Delete", JOptionPane.YES_NO_OPTION);
        if(opt==0){

            try{
                Class.forName("com.microsoft.sqlserver.jdbc.SQLServerDriver");
                String url="jdbc:sqlserver://localhost:1433;databaseName=NSBM;user=dhanith;password=lakjaya";
                Connection con = DriverManager.getConnection(url);
                int row = jTable1.getSelectedRow();
                String value = (jTable1.getModel().getValueAt(row, 0).toString());
                String query="DELETE FROM shedule where Addno="+value;
                PreparedStatement pst =con.prepareStatement(query);
                pst.executeUpdate();
                DefaultTableModel model = (DefaultTableModel)jTable1.getModel();
                model.setRowCount(0);
                show_shedule();

                JOptionPane.showMessageDialog(null,"Deleted Successfully");
            }
            catch(Exception ex){
                JOptionPane.showMessageDialog(this, ex.getMessage());
            }
        }

    }                                        

    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        jComboBox1.setSelectedIndex(0);
        jComboBox2.setSelectedIndex(0);
        jComboBox3.setSelectedIndex(0);

        jTextField2.setText("");
        jTextField5.setText("");

        jTextField3.setText("");

        jTextField4.setText("");

        jTextField6.setText("");
    }                                        

    private void jTable1MouseClicked(java.awt.event.MouseEvent evt) {                                     

        int i = jTable1.getSelectedRow();
        TableModel model = jTable1.getModel();

        String batch = model.getValueAt(i, 1).toString();
        switch(batch){
            case"18.1":
            jComboBox1.setSelectedIndex(0);
            break;
            case"18.2":
            jComboBox1.setSelectedIndex(1);
            break;
            case"19.1":
            jComboBox1.setSelectedIndex(2);
            break;
            case"19.2":
            jComboBox1.setSelectedIndex(3);
            break;
            case"20.1":
            jComboBox1.setSelectedIndex(4);
            break;
            case"20.2":
            break;
        }

        String year = model.getValueAt(i, 2).toString();
        switch(year){
            case"Year 1":
            jComboBox2.setSelectedIndex(0);
            break;
            case"Year 2":
            jComboBox2.setSelectedIndex(1);
            break;
            case"Year 3":
            jComboBox2.setSelectedIndex(2);
            break;
            case"Year 4":
            jComboBox2.setSelectedIndex(3);
            break;

        }
        String faculty = model.getValueAt(i, 3).toString();
        switch(faculty){
            case"Computing":
            jComboBox3.setSelectedIndex(0);
            break;
            case"Business":
            jComboBox3.setSelectedIndex(1);
            break;
            case"Engineering":
            jComboBox3.setSelectedIndex(2);
            break;

        }

        jTextField2.setText(model.getValueAt(i, 4).toString());

        jTextField5.setText(model.getValueAt(i, 5).toString());
        jTextField3.setText(model.getValueAt(i, 6).toString());

        try{
            int srow = jTable1.getSelectedRow();
            //object of date
            Date date = new SimpleDateFormat("yyyy-MM-dd").parse((String)model.getValueAt(srow, 7));
            jDateChooser1.setDate(date);
        }
        catch(Exception ex){
            JOptionPane.showMessageDialog(this, ex.getMessage());

        }

        jTextField4.setText(model.getValueAt(i, 8).toString());

        jTextField6.setText(model.getValueAt(i, 9).toString());
    }                                    

    private void jTextField4ActionPerformed(java.awt.event.ActionEvent evt) {                                            
        // TODO add your handling code here:
    }                                           

    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {                                         
        try{
            Class.forName("com.microsoft.sqlserver.jdbc.SQLServerDriver");
            String url="jdbc:sqlserver://localhost:1433;databaseName=NSBM;user=dhanith;password=lakjaya";
            Connection con = DriverManager.getConnection(url);//Database Connection
            //insert statement
            String query = "insert into shedule(batch, year, faculty, module, lecturer, time, date, duration, hall)values(?,?,?,?,?,?,?,?,?)";
            //preparedstatement and pass query
            PreparedStatement pst = con.prepareStatement(query);

            //get the test from user and insert
            String bat=jComboBox1.getSelectedItem().toString();
            pst.setString(1, bat);

            String yea=jComboBox2.getSelectedItem().toString();
            pst.setString(2, yea);

            String fac=jComboBox3.getSelectedItem().toString();
            pst.setString(3, fac);

            pst.setString(4, jTextField2.getText() );

            pst.setString(5, jTextField5.getText() );

            pst.setString(6, jTextField3.getText() );

            SimpleDateFormat sdf = new SimpleDateFormat("yyyy-MM-dd");
            //declare a string variable store the date
            String date =sdf.format(jDateChooser1.getDate()) ;
            //save the string in database
            pst.setString(7,date);

            pst.setString(8, jTextField4.getText() );

            pst.setString(9, jTextField6.getText() );
            show_shedule();
            //executeupdate
            pst.executeUpdate();
            //display message
            JOptionPane.showMessageDialog(null, "Inserted Sucessfully");

        }
        catch(Exception ex){
            JOptionPane.showMessageDialog(this, ex.getMessage());

        }
    }                                        

